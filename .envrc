#!/bin/bash
# Auto-activate venv when entering AgentOS directory
source venv/bin/activate

# Optioneel: export environment variables
export PYTHONPATH="${PWD}:${PYTHONPATH}"
export AGENTOS_ROOT="${PWD}"

# Welkom bericht
echo "🚀 AgentOS development environment activated!"
echo "📍 Virtual env: $(which python)"
echo "📁 Project root: ${PWD}"